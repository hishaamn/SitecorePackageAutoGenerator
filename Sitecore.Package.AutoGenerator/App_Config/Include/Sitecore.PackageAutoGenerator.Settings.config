<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <settings>
      <!--
        This setting is to set the folder name of the stored package generator projects. Default: PackageGenerator
      -->
      <setting name="PackageGeneratorFolder" value="PackageGenerator" />

      <!--
        This setting is to set the source database. Default: master
      -->
      <setting name="SourceDatabase" value="master" />
    </settings>
  </sitecore>
</configuration>